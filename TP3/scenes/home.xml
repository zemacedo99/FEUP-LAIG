                <rotation axis="x" angle="-20"/>
                <rotation axis="y" angle="0"/>
                <rotation axis="z" angle="0"/>
                <scale sx="1.0" sy="1.0" sz="1.0"/>
            </keyframe>
            <keyframe instant="2.9"> <!-- Pre Topo (salto) -->
                <translation x="0.0" y="-6.0" z="-2.0"/>
                <rotation axis="x" angle="-20"/>
                <rotation axis="y" angle="0"/>
                <rotation axis="z" angle="0"/>
                <scale sx="1.0" sy="1.0" sz="1.0"/>
            </keyframe>
            <keyframe instant="3.0"> <!-- Topo (salto) -->
                <translation x="0.0" y="-6.0" z="-2.0"/>
                <rotation axis="x" angle="-20"/>
                <rotation axis="y" angle="0"/>
                <rotation axis="z" angle="0"/>
                <scale sx="1.0" sy="1.0" sz="1.0"/>
            </keyframe>
            <keyframe instant="3.1"> <!-- Pos Topo (salto) -->
                <translation x="0.0" y="-6.0" z="-2.5"/>
                <rotation axis="x" angle="-20"/>
                <rotation axis="y" angle="0"/>
                <rotation axis="z" angle="0"/>
                <scale sx="1.0" sy="1.0" sz="1.0"/>
            </keyframe>

            <keyframe instant="4.0"> <!-- Solo (fim salto) -->
                <translation x="0.0" y="-10.0" z="-5.0"/>
                <rotation axis="x" angle="-40"/>
                <rotation axis="y" angle="0"/>
                <rotation axis="z" angle="0"/>
                <scale sx="1.0" sy="0.8" sz="1.0"/>
            </keyframe>

            <keyframe instant="6.0"> <!-- Rolamento -->
                <translation x="0" y="-10.0" z="-8.0"/>
                <rotation axis="x" angle="-30"/>
                <rotation axis="y" angle="0"/>
                <rotation axis="z" angle="0"/>
                <scale sx="1" sy="1" sz="1"/>
            </keyframe>

            <keyframe instant="8.0"> <!-- Fim -->
                <translation x="0" y="-10.0" z="-10.0"/>
                <rotation axis="x" angle="-60"/>
                <rotation axis="y" angle="0"/>
                <rotation axis="z" angle="0"/>
                <scale sx="0.8" sy="0.8" sz="0.8"/>
            </keyframe>

        </animation>

